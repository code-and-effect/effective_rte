<script type="text/javascript">
  $('#<%= dom_id %>').wymeditor(
    {<%= raw opts.to_json[1..-2] %>,
      postInit: function(wym) {
        var wymInsertAssets = wym.insert_assets({"dialog_url" : "<%= opts[:assetDialogUrl] %>"});
        wymInsertAssets.init(wym);

        var wymHtmlCleanup = wym.html_cleanup();
        wymHtmlCleanup.init(wym);
      }
    }
  );
</script>
